program test_poseidon8_matching.aleo;

function test_hash_computation:
    cast aleo1yu926k0jqqzfv06js4jlsxnf2ejah47rfqsxmwfx6tvuxxgvrqpqdlq5y0 into r0 as field;
    cast 1000000u64 into r1 as field;
    assert.eq r0 947113542301545363857217716479361902320907642358148838248532178752253004327field;
    assert.eq r1 1000000field;
    cast r0 into r2 as [field; 1u32];
    hash.psd8 r2 into r3 as field;
    cast r1 into r4 as [field; 1u32];
    hash.psd8 r4 into r5 as field;
    cast 28502265869609713379017890711600558823field into r6 as [field; 1u32];
    hash.psd8 r6 into r7 as field;
    assert.eq r3 1868191709334896628112654988189468947525204652525207010811605775620377366748field;
    assert.eq r5 7356817805177595054700421685841010225912334794880219079272107830218845198937field;
    assert.eq r7 3578469920779271006144279877385411985508786201932034380022468345564585491547field;
    add r3 r5 into r8;
    add r8 r7 into r9;
    assert.eq r9 4359017685863392264708531612634344627570426314183396642170948495486398818191field;
